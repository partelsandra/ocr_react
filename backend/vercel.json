{
    "functions": {
      "api": {
        "source": "app.py",
        "runtime": "python3.8"
      }
    },
    "routes": [
      {
        "src": "/upload",
        "dest": "/api/upload"
      },
      {
        "src": "/process",
        "dest": "/api/process"
      },
      {
        "src": "/backend/saved_images/(.*)",
        "dest": "/api/uploaded_file"
      }
    ]
  }
  